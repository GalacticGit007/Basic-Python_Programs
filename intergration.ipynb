    "#Normal Integration\n",
    "from numpy import cos, pi, sin, exp\n",
    "from scipy.integrate import quad\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def f(x):\n",
    "    return exp(cos(-2*x*pi)*sin(x))+3.2\n",
    "\n",
    "sci_res,sci_err = quad(f,-2,2)\n",
    "\n",
    "print(\"The result is \",sci_res,sci_err)"
   ]
  }
